	<div class="tab-content devicen">
     <div class="tab-pane active fade in" id="login-block">
      <!-- Login Block Form -->
       <div class="login-block-form">		
			<h2 class="padfornav">Reenvio instrucciones de desbloqueo</h2>

			<%= form_for(resource, as: resource_name, url: unlock_path(resource_name), html: { method: :post }) do |f| %>
			  <%= devise_error_messages! %>

			  <div class="field form-group">
			    <%= f.label :Correo %><br />
			    <%= f.email_field :email, autofocus: true, :placeholder => "Introduce tu Correo", :class => 'form-control'  %>
			  </div>

			  <div class="actions">
			    <%= f.submit "confirmar" %>
			  </div>
			<% end %>

			<%= render "devise/shared/links" %>
      </div>
   </div>
</div>

